  # 分页
  pod 'DNSPageView', '~> 1.0.1'
  #跑马灯
  pod 'JXMarqueeView'
  #滚动页
   pod 'LTScrollView'
  pod 'MJRefresh'
  #pod 'SkeletonView'
  #消息提示
  pod 'SwiftMessages'
  #pod 'YYKit'
  pod 'SVProgressHUD'
